<UserControl x:Name="userControl" x:Class="Sky_Guide.PagingControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Sky_Guide"
             mc:Ignorable="d" 
             Width="Auto" Height="Auto">
    <Grid>
        <ComboBox x:Name="comboBox" ItemTemplate="{DynamicResource PagingDataTemplate}" VerticalAlignment="Center" Width="80" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Hidden" MaxDropDownHeight="98" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="15" Style="{DynamicResource ComboBoxStyle1}" HorizontalAlignment="Center" ItemContainerStyle="{DynamicResource ComboBoxItemStyle1}" SelectionChanged="comboBox_SelectionChanged"/>
    </Grid>
</UserControl>
